import math
def add(num1, num2):
    return (num1 + num2)

def divide(num1, num2):
    return (num1 / num2)

def cut(num1, num2):
    return (num1 - num2)

def multiply(num1, num2):
    return (num1 * num2)
def pow(num1, num2):
    return math.pow(num1, num2)
for x in range(100):
    num1 = int(input("num1:"))
    num2 = int(input("num2:"))

    print(add(num1, num2))
    print(divide(num1, num2))
    print(cut(num1, num2))
    print(multiply(num1, num2))
    print(pow(num1, num2))
